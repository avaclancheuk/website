    "uglify": "^0.1.5"
  },
  "devDependencies": {
    "eslint": "^2.1.0",
    "eslint-config-clock": "^1.1.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.1.0",
